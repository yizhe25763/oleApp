<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include
        android:id="@+id/choose_discount_coupon_list_title_layout"
        layout="@layout/layout_ole_title" />

    <ListView
        android:id="@+id/choose_discount_coupon_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/choose_discount_coupon_list_title_layout"
        android:layout_margin="@dimen/w15"
        android:background="@color/transparent"
        android:cacheColorHint="@android:color/transparent"
        android:divider="@android:color/transparent"
        android:dividerHeight="@dimen/h10"
        android:drawSelectorOnTop="true"
        android:scrollbars="none" />


</RelativeLayout>

